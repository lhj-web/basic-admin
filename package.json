{
  "name": "basic-admin",
  "version": "0.0.6",
  "author": {
    "name": "Lovell Liu",
    "email": "yufengjisuo@gmail.com",
    "url": "https://github.com/lhj-web"
  },
  "license": "MIT",
  "homepage": "https://github.com/lhj-web/basic-admin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lhj-web/basic-admin.git"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "scripts": {
    "dev": "vite",
    "serve": "npm run dev",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/scripts/postBuild.ts",
    "build:no-cache": "pnpm clean:cache && npm run build",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint:lint-staged": "lint-staged",
    "lint:eslint": "eslint --cache --max-warnings 0  \"src/**/*.{vue,ts,tsx}\" --fix",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && pnpm i",
    "gen:icon": "esno ./build/generate/icon/index.ts"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons-vue": "^6.1.0",
    "@iconify/iconify": "^2.2.1",
    "@vue/runtime-core": "^3.2.40",
    "@vue/shared": "^3.2.40",
    "@vueuse/core": "7.3.0",
    "ant-design-vue": "3.1.1",
    "axios": "0.24.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.5",
    "echarts": "^5.4.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.0.22",
    "qs": "^6.11.0",
    "showdown": "^1.9.1",
    "sortablejs": "^1.15.0",
    "vditor": "^3.8.17",
    "vue": "^3.2.40",
    "vue-i18n": "9.1.9",
    "vue-router": "^4.1.5",
    "vue-types": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@iconify/json": "2.0.9",
    "@intlify/vite-plugin-vue-i18n": "^5.0.1",
    "@lovelliu/eslint-config": "^0.0.1",
    "@purge-icons/generated": "^0.8.1",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.63",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.40",
    "autoprefixer": "^10.4.12",
    "chalk": "^5.0.1",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "esno": "^0.14.1",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "less": "^4.1.3",
    "lint-staged": "12.1.2",
    "postcss": "8.4.4",
    "postcss-html": "^1.5.0",
    "postcss-less": "^5.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unocss": "^0.45.26",
    "vite": "^2.9.15",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-purge-icons": "^0.8.2",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-svg-icons": "^1.1.0",
    "vite-plugin-theme": "^0.8.6",
    "vite-plugin-vue-setup-extend": "^0.1.0",
    "vue-tsc": "^1.0.2"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "^2.56.3",
    "gifsicle": "5.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix"
    ],
    "*.md": [
      "eslint --fix"
    ]
  }
}
